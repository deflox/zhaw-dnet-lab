<Window x:Class="DN3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DN3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="GetUrl" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="Button_Click" Width="62"/>
        <TextBox HorizontalAlignment="Left" Margin="99,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120">
            <Binding Path="Url" Mode="TwoWay"/>
        </TextBox>
        <Label HorizontalAlignment="Left" Margin="10,49,0,0" VerticalAlignment="Top" Width="116">
            <Binding Path="Size" Mode="OneWay"/>
        </Label>
        <Label HorizontalAlignment="Left" Margin="159,49,0,0" VerticalAlignment="Top" Width="150">
            <Binding Path="Time" Mode="OneWay"/>
        </Label>

    </Grid>
</Window>
